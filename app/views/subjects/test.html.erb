<div class="container">
  <% @chunks.each do |c| %>
  <div class="well text-center">
    <h3><%= c.question %></h3>
    <p class="hidden"><%= c.answer %></p>
      <button class="btn btn-primary btn-lg">Show Answer</button>
      <button class="btn btn-success btn-lg hidden">Correct</button>
      <button class="btn btn-default btn-lg hidden">Not Correct</button>
  </div>
  <% end %>
</div>
